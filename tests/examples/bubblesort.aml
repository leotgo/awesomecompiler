int vetor[10];

int bubblesort()
{	
	int i;
	int j;

	i = 10;

	while ( i >= 1 ) do 
	{
		j = 0;
		while ( j < i ) do 
		{
			if (vetor[j] > vetor[j + 1]) then {
				int temp;
				temp = vetor[j];
				vetor[j] = vetor[j + 1];
				vetor[j + 1] = temp;
			} ;
			j = j + 1;
		} ;
		i = i - 1;
	} ;

	return 0;
}
